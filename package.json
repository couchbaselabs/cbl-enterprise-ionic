{
  "name": "cblite-ionic",
  "version": "3.1.0",
  "description": "Plugin for Couchbase Lite Enterprise Edition for Ionic Capacitor apps",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Plugin/",
    "CouchbaseLiteEeIonic.podspec"
  ],
  "author": "Deveveloper Integrations  @ Couchbase",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/couchbaselabs/cbl-enterprise-ionic.git"
  },
  "bugs": {
    "url": "https://github.com/couchbaselabs/cbl-enterprise-ionic/issues"
  },
  "keywords": [
    "couchbase",
    "couchbase-lite",
    "capacitor",
    "plugin",
    "native"
  ],
  "scripts": {
    "verify": "npm run verify:ios && npm run verify:android",
    "verify:ios": "cd ios && pod install && xcodebuild -workspace Plugin.xcworkspace -scheme Plugin -destination generic/platform=iOS && cd ..",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "lint": "npm run eslint && npm run prettier -- --check && npm run swiftlint -- lint",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint -- autocorrect --format",
    "eslint": "eslint . --ext ts",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "docgen": "docgen --api IonicCouchbaseLitePlugin --output-readme README.md --output-json dist/docs.json",
    "build": "npm run clean && tsc && rollup -c rollup.config.js",
    "buildAll": "cd cblite && npm run clean && npm install && npm run build && cd .. && npm run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "cblite": "file:cblite",
    "@capacitor/android": "^5.7.4",
    "@capacitor/core": "^5.7.4",
    "@capacitor/docgen": "^0.0.18",
    "@capacitor/ios": "^5.7.4",
    "@ionic/eslint-config": "^0.3.0",
    "@ionic/prettier-config": "^1.0.1",
    "@ionic/swiftlint-config": "^1.1.2",
    "eslint": "^7.11.0",
    "prettier": "~2.3.0",
    "prettier-plugin-java": "~1.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.0",
    "swiftlint": "^1.0.1",
    "tslib": "^2.6.2",
    "typescript": "5.4.2"
  },
  "peerDependencies": {
    "@capacitor/core": "^5.7.4"
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6"
  }
}
